      -
        internal_properties : 
        code : |1+
         procedure MyWindow(gKanbanControl is Control, gCard kbCard)
      -
        internal_properties : 
        code : |1+
         procedure DemandeMajUI()
         
         
         STC_TITLE	= gCard.Title
         STC_TEXT	= RTFToText(gCard.Text)
         STC_DATE	= gCard.Date
         
         // Hides contributors by default
         (STC_CONTRIBUTOR1.Visible, STC_CONTRIBUTOR2.Visible, STC_UserOther.Visible) = (False, False, False)
         
         i is int                    = 1
         for each string _contributor of gCard.Contributor separee by CST_SEPARATOR
         	switch i
         		case 1
         			FillContributorBadge(STC_CONTRIBUTOR1, _contributor)
         		case 2 
         			FillContributorBadge(STC_CONTRIBUTOR2, _contributor)
         			
         		other case
         			STC_UserOther.Visible = True
         	end
         	i++
         end
         
         EDT_Tags = ""
         EDT_Tags.garrTokens.SupprimeTout()
         i = 1
         nCardID is 8-byte int = gCard.Extra
         for each Tag_KanbanCard where KanbanCardID = nCardID
         	ProjectTag.ReadSeekFirst(ProjectTagID,Tag_KanbanCard.ProjectTagID)
         	
         	aToken is Token
         	aToken.Caption		= ProjectTag.TagName
         	aToken.Value		= ProjectTag.ProjectTagID
         	aToken.BackgroundColor	= ProjectTag.Color
         	aToken.Color		= ReadableColor(aToken.BackgroundColor)
         	
         	let n = EDT_Tags.garrTokens.ajoute(aToken)
         	
         	EDT_Tags.AddToken(EDT_Tags.garrTokens[n])
         	
         	i++
         end	
         
         // Shows the tags, if any 
         LAYOUT_CARD[2].Visible	= (i <> 1)
         
         // Shows the text, if any
         LAYOUT_CARD[3].Visible	= (STC_TEXT <> "")
         
         	INTERNAL PROCEDURE displayBadge(_cBadge champ, _sUsername string)
         		_cBadge..Visible		= True
         		_cBadge				= Left(_sUsername,3).Upper()
         		_cBadge.BackgroundColor	= getUserColor(_sUsername)
         		_cBadge.Color		= ReadableColor(_cBadge.BackgroundColor)
         		
         	end
        type : 234
