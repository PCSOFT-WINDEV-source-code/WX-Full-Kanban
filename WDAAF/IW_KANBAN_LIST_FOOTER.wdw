     code : |1+
      procedure DragCard_Drop(Card ) //is kbCard)
      
      // executes the add process and cancels if it returns False
      if ExecuteProcess(gKanbanControl,trtDeleteKanbanCard_, Card) = False then
      	// cancel
      	return
      end
      
      
      // Delete card
      nIndex is int =  Card.Index
      gListCard.SupprimeCarte(nIndex) 
      
      ToastDisplay("Card deleted")
      
