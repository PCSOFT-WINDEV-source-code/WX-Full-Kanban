        height : 44
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           internal_properties : 
           code : |1+
            // If the card title has been specified
            if EDT_CARD_TITLE = "" then return
            // Adds a new card to the list
            NewCard is kbCard
            NewCard.Title			= EDT_CARD_TITLE
            
            // executes the add process and cancels if it returns False
            if ExecuteProcess(gKanbanControl,trtAddKanbanCard_, NewCard, gListCard) = False then
            	// cancel
            	EDT_CARD_TITLE			= ""
            	IW_KANBAN_LIST_FOOTER.Plane	= PLANE_ADD_CARD
            	return
            end
            // add 
            NewCardIndex is int = gListCard.AjouteCarte(NewCard)
            if NewCardIndex > 0 then
            	// added successfully
            	EDT_CARD_TITLE			= ""
            	IW_KANBAN_LIST_FOOTER.Plane	= PLANE_ADD_CARD
            	// scroll so that the new card is visible
            	gListCard.PositionListe(gListCard.Card[NewCardIndex])
            	<if ExecutionTarget = Android or ExecutionTarget = iOS>
            		SIPVisible(False)	
            	<end>
            end
            
           type : 18
