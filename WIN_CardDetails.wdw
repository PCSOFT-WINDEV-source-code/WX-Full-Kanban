     name : FillContributors
     internal_properties : CAAAAAgAAAAmB8eZRM/fG1TvceEHtxU1zI3sj4jPS7MtmyH/Hdydaix/1KIYh1nCkD3Nuq87D15WUAIqcWS1TsAAOb2klfwQXBy5F4FnJeTaX60Ik6hMUKRFg6d+3ew4sKEf8Bylm6oAtoLrPzBvzaJhmjv/5jGWjQe4NovEjs0A6f1ElfI=
     procedure_id : 1446906424105933771
     type_code : 14
     code : |1-
      // Populates the contributors Looper control
      procedure FillContributors()
      
      
      // Fill contributors
      for each string _sContributor of kanbancard.Contributor separee by CST_SEPARATOR
      	addContributor(_sContributor)
      end                                                                                                  
      
      // sorts contributors by username
      LOOP_Contributors.Sort("ATT_Username")
      
      // Add the add button, use "zzz" as username to always be the last result
      let nEnd = addContributor(CST_USERNAME_ADD)
      LOOP_Contributors[nEnd].IMG_Add.Visible = True
      
      LOOP_Contributors.SelectMinus()
     type : 458752
