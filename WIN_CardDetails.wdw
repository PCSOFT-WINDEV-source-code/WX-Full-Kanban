   -
     code : |1+
      procedure MyWindow(gkbCard is kbCard, gnProjectID 8-byte int)
      
      MySelf.ReturnedValue = False
      
      // Sets the position to display the card data stored in the database
      KanbanCard.ReadSeekFirst(KanbanCardID, gkbCard.Extra)
      if not KanbanCard.Found() then
      	Error("Card #[% gkbCard.Extra %] not found")
      	Close(MyWindow, True)
      end
      
      
