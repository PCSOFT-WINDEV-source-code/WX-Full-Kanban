  procedures :
   -
     name : addContributor
     procedure_id : 1446906424105737163
     type_code : 14
     code : |1+
      procedure addContributor(sUsername string)
      nRow is int 
      
      // Don't add contributor if already in the list
      FOR EACH ROW of LOOP_Contributors
      	if ATT_Value = sUsername then
      		return 0
      	end
      end
      
      nRow = LOOP_Contributors.AddLine(sUsername, sUsername)
      
      // Displays the badge with a given color
      FillContributorBadge(LOOP_Contributors[nRow].STC_User1, sUsername)
      
      // Hides interaction images
      LOOP_Contributors[nRow].IMG_Add.Visible = False
      LOOP_Contributors[nRow].IMG_Delete.Visible = False
      
      return nRow
     type : 458752
