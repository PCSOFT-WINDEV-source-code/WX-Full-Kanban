        type : 14
      -
        code : |1-
         if EDT_Title ~= "" then
         	Error("The title cannot be empty")
         	return
         end
         
         
         // Assigns values to the card
         ScreenToFile(WIN_CardDetails, KanbanCard)
         
         for each Tag_KanbanCard where KanbanCardID = kanbancard.KanbanCardID
         	Tag_KanbanCard.Delete()
         end
         
         for each token of EDT_Tags.Token
         	Tag_KanbanCard.KanbanCardID	= kanbancard.KanbanCardID	
         	Tag_KanbanCard.ProjectTagID	= token.valeur	
         	Tag_KanbanCard.Add()
         end
         
         
         // Gets the list of contributors
         kanbancard.Contributor = ""
         FOR EACH ROW of LOOP_Contributors
         	
         	// The element stored with the "ZZZ" username corresponds to the Add button (always last)
         	if ATT_Username in("",CST_USERNAME_ADD) then continue
         	
         	// Handles a separator
         	if kanbancard.Contributor <> "" then
         		kanbancard.Contributor += CST_SEPARATOR
         	end
         	
         	kanbancard.Contributor+= ATT_Username
         end
         
         
         
         // Gets the description in Markdown format
         kanbancard.Text = IWC_Editor
         
         // Saves the card
         KanbanCard.Save()
         
         Close(MyWindow, True)
