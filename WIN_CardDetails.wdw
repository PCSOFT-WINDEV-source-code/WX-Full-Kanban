        code : |1+
         // Version 1
         // Description 
         // Multi-selection with tokens
         
         global
         // Define the possible values for multi-selection
         garrTokens is array of Token
         
         local
         for each ProjectTag where KanbanProjectID = gnProjectID
         	aToken is Token
         	aToken.Caption = ProjectTag.TagName
         	aToken.Value = ProjectTag.ProjectTagID
         	aToken.BackgroundColor = ProjectTag.Color
         	aToken.Color = ReadableColor(aToken.BackgroundColor)
         	
         	let n = garrTokens.Add(aToken)
         	
         	if Tag_KanbanCard.ReadSeekFirst(ProjectTagID_KanbanCardID, [ProjectTag.ProjectTagID, kanbancard.KanbanCardID]) then
         		MyControl.AjouteJeton(garrTokens[n])
         	end
         end
        type : 14
