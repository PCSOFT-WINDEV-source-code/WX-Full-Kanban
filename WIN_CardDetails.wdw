         	// Iterates over the subcontrols to find the buttons and change the color
          	INTERNAL PROCEDURE changeButtonColor(_cControl)
         		i			is int	= 1
         		cBtn		is Control
         		sResControl	is string	= EnumControl(_cControl,i)
         		while sResControl <> "" 
         			
         			if sResControl.StartsWith("BTN_") then
         				cBtn <- {sResControl, indControl}
         				cBtn.BackgroundColor = RGB(114, 116, 118)
         				cBtn.border.color = RGB(114, 116, 118)
         			end
         			
         			i ++
         			sResControl = EnumControl(_cControl,i)
         		end
         		
         	end
         
