               // Gets the list and the card under the cursor before opening the context menu
               posX,posY		are int	= (MouseXPos(), MouseYPos())
               
               cCurrentList	<- Null
               kbCurrentCard	= Null
               
               nListIndex	is int		= KanbanInfoXY(KANBAN_Project, kbListInfo, posX, posY)
               nIndexCard	is int		= KanbanInfoXY(KANBAN_Project,kbCardInfo, posX, posY)
               
               // If not even in a list, nothing to display
               if nListIndex = -1 then
               	return True
               end
               
               cCurrentList <- KANBAN_Project.Liste[nListIndex]
               
               if nIndexCard > 0 then
               	kbCurrentCard <- KANBAN_Project.liste[nListIndex].Carte[nIndexCard]
               end
               
               // Defines the context menu to be displayed
               if kbCurrentCard <> Null then
               	KANBAN_Project.ContextMenu = "MENU_Card"
               else
               	KANBAN_Project.ContextMenu = "MENU_Kanban"
               end
