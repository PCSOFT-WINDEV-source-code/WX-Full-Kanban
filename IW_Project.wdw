        code : |1+
         procedure MyWindow(gnProjectID 8-byte int, gcallbacks stCallbacks)
         
         local
         // Checks that the Kanban exists and displays the title
         HReadSeekFirst(KanbanProject, KanbanProjectID, gnProjectID)
         if not HFound(KanbanProject) then
         	Error("Unable to find the project with ID #[% gnProjectID %]")
         	Close("")
         end
         
         // Changes the title of the window
         STC_Title = KanbanProject.ProjectName
         
         kbDisplayLists(KANBAN_Project, gnProjectID)	
      -
        code : |1+
         procedure MaProcedure(bResetLists boolean = False)
         
         if bResetLists then
         	KANBAN_Project.Visible = False
         	
         	kbDisplayLists(KANBAN_Project, gnProjectID)	
         	
         	KANBAN_Project.Visible = True
         end
         
         KANBAN_Project.Display()
