    options :
     -
       name : OPT_Right
       identifier : 0x241471c0501eb241
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1+
            cKanban			is Control	<- KANBAN_Project
            cList			is Control	<- KANBAN_Project.cCurrentList
            
            nIndex			is int	= KanbanListIndex(cKanban,cList)
            
            nNextIndex	is int	= nIndex + 1
            
            // No swap if positioned on the rightmost list
            if nIndex >= cKanban.List..Count then
            	return
            end
            
            // Get list to be swapped
            cNextList is Control <- cKanban.List[nNextIndex]
            
            
            // Swaps positions in the database
            kbSwapListPositions(cList.Extra.ID, cNextList.Extra.ID)
            
            // Move in the control
            KanbanMoveList(cKanban,nIndex, nNextIndex)
            
            
           type : 32
     -
       name : OPT_Left
       identifier : 0x241471c0501fb241
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1+
            cKanban				is Control	<- KANBAN_Project
            cList				is Control	<- KANBAN_Project.cCurrentList
            
            nIndex				is int	= KanbanListIndex(cKanban,cList)
            
            nPreviousIndex	is int	= nIndex - 1
            
            // No swap if positioned on the rightmost list
            if nIndex <= 1 then
            	return
            end
            
            // Get list to be swapped
            cPreviousList is Control <- cKanban.List[nPreviousIndex]
            
            // Swaps positions in the database
            kbSwapListPositions(cList.Extra.ID, cPreviousList.Extra.ID)
            
            // Move in the control
            KanbanMoveList(cKanban,nIndex, nPreviousIndex)
            
           type : 32
     -
       name : OPT_Edit_list
       identifier : 0x241471c05020b241
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1+
            cList			is Control	<- KANBAN_Project.cCurrentList
            
            sListTitle		is string	= cList.title
            nMaxNbElements	is int	= cList.Extra.MaxNbElements
            
            if not Open(WIN_ListDetails, sListTitle, nMaxNbElements) then
            	return
            end
            
            cList.Title				= sListTitle
            cList.Extra.MaxNbElements	= nMaxNbElements	
            cList.MaxNbCard			= nMaxNbElements	
            
            kbEditList(cList.Extra.ID, sListTitle, nMaxNbElements)
           type : 32
     -
       name : OPT_Delete_list
       identifier : 0x241471c05021b241
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1+
            cList is Control <- KANBAN_Project.cCurrentList
            
            if YesNo("Delete the list and its associated cards?") = No then
            	return
            end
            
            if not KBDeleteList(cList.Extra.ID) then
            	return
            end
            
            KanbanDeleteList(KANBAN_Project, KanbanListIndex(KANBAN_Project,cList.Name))
            
