     code : |1-
       procedure MyWindow()
      
      STC_WinTitle = "Welcome [% gUser.login %]!"
      
      
      // Callbacks to be passed to the internal window
      gstCallbacksIW is stCallbacks
      gstCallbacksIW.onDeleteProject = KanbanDeletedProject
      gstCallbacksIW.onModifyProject = KanbanModifiedProject
   -
     code : |1+
      LOOP_Project.DeleteAll()
      
      // Shows the projects associated with the user
      DisplayProjects()
      
      LOOP_Project.SelectMinus()
      
