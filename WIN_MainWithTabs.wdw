  procedures :
   -
     name : DisplayProjects
     internal_properties : CAAAAAgAAABGB3cgv7oy1RLUxfzr/a94YIcCL2JxZXSVqeR4vI0I5apl1du7Hb3cxcDlnRRMPt9YXWFCKuoQJ6ZEaWVEB8bJxBISm/U7Kl0aLdsIGNcssb6ziqFmYfa7mfFmNSjnlGMGG20wV1xThLWwYnmd
     procedure_id : 1446906436993591375
     type_code : 14
     code : |1+
      procedure DisplayProjects()
      nRow is int
      
      LOOP_Project.DeleteAll()
      
      // Prepare the queyr
      QRY_ListUserProjects.ParamUsername = gUser.Login
      QRY_ListUserProjects.ExecuteQuery()
      
      
      // Iterates over the projects associated with the users
      for each QRY_ListUserProjects 
      	
      	// Adds the row
      	nRow = LOOP_Project.AddLine(QRY_ListUserProjects.KanbanProjectID, QRY_ListUserProjects.ProjectName)
      	
      	i			is int	= 1
      	cBadge	is Control
      	
      	// Hide all badges in the row by default
      	for i = 1 _to_ 3
      		cBadge <- {"LOOP_Project["+nRow+"].STC_User"+i,indControl}
      		cBadge.Visible = False
      	end
      	LOOP_Project[nRow].STC_UserOther.Visible	= False
      	
      	i												= 1
      	
      	// Handles badges of users associated with the project, up to three
      	for each ProjectUser where KanbanProjectID = QRY_ListUserProjects.KanbanProjectID
      		
      		// Shows the users
      		switch i
      			case <= 3
      				cBadge 				<- {"LOOP_Project["+nRow+"].STC_User"+i,indControl}
      				
      				FillContributorBadge(cBadge, ProjectUser.Username)
      
      				
      			// If more than three users
      			other case
      				LOOP_Project[nRow].STC_UserOther.Visible = True
      		end
      		
      		// Displays the project author and creation date
      		LOOP_Project[nRow].ATT_Creation = "Created by [% QRY_ListUserProjects.AuthorUsername %] on [% DateVersChaîne(QRY_ListUserProjects.DateCreated) %]"
      		
      		i ++
      	end	
      end
      
     type : 458752
   -
     name : opensTab
     procedure_id : 1446906436993656911
     type_code : 14
     code : |1+
      procedure opensTab(nID 8-byte int, sTitle string)
      
      MyTab	is Control 
      sAlias		is string
      i			is int	= 1
      
      // Checks if the project is already open in a tab, if so displays it
      sAlias = "Alias_"+nID
      
      if TAB_Main.Status(sAlias) = paneNotFound then // Opens the tab and sets the project ID as alias
      	
      	MyTab <- TAB_Main.Open(sTitle, IW_Project, nID, gstCallbacksIW)
      	MyTab..Alias = sAlias
      
      else // If the tab exists, set the position on it
      	TAB_Main = sAlias	
      end
      
     type : 458752
