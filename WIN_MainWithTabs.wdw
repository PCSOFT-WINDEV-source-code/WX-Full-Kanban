     name : KanbanDeletedProject
     internal_properties : CAAAAAgAAACXYYFZN5s3Oq69yDK9oYxj0IXNuDEm9KmFd/a62NXPJ8HkVuDrCNl9+EMDBJCe9e5/CjpwEHTeEJKZHPGshHLQua1Qp8EgtJS4+lZAovbMtrnwTCkGcNrcDea5KQqArtZHjumq1WnRF6e6azA=
     procedure_id : 1446906436993722447
     type_code : 14
     code : |1+
      // Summary: Callback to be called by the internal windows when the project is deleted
      // Syntax:
      //KanbanDeletedProject (<nID> is 8-byte int)
      //
      // Parameters:
      //	nID (8-byte integer): ID of the deleted board
      // Return value:
      // 	None
      //
      procedure KanbanDeletedProject(nID 8-byte int)
      
      TAB_Main.Close(""+nID)
      MyWindow.RequestRefreshUI()
     type : 458752
   -
     name : KanbanModifiedProject
     internal_properties : CAAAAAgAAACXYYFZN5s3Oq69yDK9oYxj0IXNuDEm9KmFd/a62NXPJ8HkVuDrCNl9+EMDBJCe9e5/CjpwEHTeEJKZHPGshHLQua1Qp8EgtJS4+lZAovbMtrnwTCkGcNrcDea5KQqArtZHjumq1WnRF6e6azA=
     procedure_id : 1446906436993787983
     type_code : 14
     code : |1-
      // Summary: Callback called when a project is modified
      // Syntax:
      //KanbanModifiedProject (<nID> is 8-byte int, <sNewName> is string)
      //
      // Parameters:
      //	nID (8-byte integer): ID of the modified project
      //	sNewName (ANSI string): New project name to be displayed
      // Return value:
      // 	None
      //
      procedure KanbanModifiedProject(nID 8-byte int, sNewName string)
      
      sAlias is string = "Alias_"+nID
      
      TAB_Main[sAlias]..Caption = sNewName
      
      RequestRefreshUI()
