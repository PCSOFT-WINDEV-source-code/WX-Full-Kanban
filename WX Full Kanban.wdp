     code : |1-
      gUser is gpwUser
      
      
      
      local
      // Manages the connection to the database
      // The procedure contains code with comments to manage a client-server HSQL connection
      if not initDatabaseConnection() then
      	Error(ErrorInfo())
      	EndProgram()
      end
      
      nRes				is int
      bFirstConnection	is boolean	= LoadParameter("FirstTimeOpening",True)
      
      // Checks if the application is launched for the first time
      if  bFirstConnection then
      	Info("This example shows the use of a Kanban control associated with a database in a project management application.",
      	"To log in for the first time, you must a create an account using SUPERVISOR as both username and password.")
      end
      
      
      // User login
      nRes = gpwOpen()
      if nRes <> gpwOk then
      	switch nRes
      		case gpwCancel : 
      		case gpwError : Error("Error while initializing the groupware."+CR+ErrorInfo())
      		case gpwUnknownUser : Error("Unknown user.")
      		case gpwInvalidPassword : Error("Invalid password.")
      	end
      	EndProgram()
      end
      
      // To no longer show the first login message
      if bFirstConnection then
      	SaveParameter("FirstTimeOpening",False)
      end
      
      // Retrieve the user information
      gUser						= gpwGetUserInfo(gpwFullDetails)
      
      // Passes the username to the internal component
      SET_Utilities.gsUsername	= gUser.Login
      
      // Structure containing the callback(s) to be passed to the internal windows of the Kanban control
      stCallbacks is Structure
      	onDeleteProject is procedure(nID 8-byte int)
      	onModifyProject is procedure(nID 8-byte int, sNom string)
      end
